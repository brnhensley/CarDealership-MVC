@using Dealership.Models;

<h1>To Car List</h1>
<ul>

@if (Model.Count == 0)
{
 <p>You have no list items</p>
}
@if(Model.Count != 0)
{
 @foreach (Car car in Model)
 {
   <li>@car.GetMakeModel(), @car.GetPrice(), @car.GetMiles(), @car.GetInfo()</li>
 }
}

</ul>
<a href="/cars/new">Add a new car.</a>
